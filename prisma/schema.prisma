generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Products {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  image      String?
  name       String?
  detail     String?
  subTypeId  String?   @db.Uuid
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  price      Int?
  SubType    SubType?  @relation(fields: [subTypeId], references: [id])
}

model SubType {
  id         String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name       String?
  TypeId     String?    @db.Uuid
  createdAt  DateTime?  @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  Products   Products[]
  Type       Type?      @relation(fields: [TypeId], references: [id])
}

model Type {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name       String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  SubType    SubType[]
}

model Contact {
  id           String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  address      String?
  tel          String?
  email        String?
  linkMap      String?
  facebook     String?
  linkFacebook String?
  line         String?
  linkLine     String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime?
  deleted_at   DateTime?
}

model About {
  id           String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  headtitle    String?
  history      String?
  subhistory   String?
  portfolio    String?
  subportfolio String?
  videotitle   String?
  video        String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime?
  deleted_at   DateTime?
  headpolicy   String?
  subpolicy    String?
}

model Policy {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  headpolicy String?
  subpolicy  String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}

model Block {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title      String?
  subtitle   String?
  detail     String?
  subdetail  String?
  sourcename String?
  sourcelink String?
  imgtitle   String?
  imgdetail  String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}
